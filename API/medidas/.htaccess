RewriteEngine On
RewriteBase /
RewriteCond %{REQUEST_FILENAME} !-f 
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^ciudad/(.+)/(.+)$ ?ciudad=$1&provincia=$2 [QSA,L,R=301]
RewriteRule ^ciudad/(.+)$ ?ciudad=$1 [QSA,L,R=301]



RewriteEngine On

RewriteRule ^([^/]+)/([^/]+) detalle.php?peticion=$1 [L,QSA]