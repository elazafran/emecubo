Options FollowSymLinks
CheckSpelling off
Options +ExecCGI

AddType text/x-server-parsed-html .shtm .shtml .htm .html
RewriteEngine On
#RewriteEngine /
RewriteRule ^obtener\.html$ operacion.php
#http://localhost/EMECUBO/API/  
RewriteRule ^([a-z]+)#([0-9]+)$ operacion.php?op=$1&op1=$2&op2=$3
#http://localhost/EMECUBO/API/dame/3/4
RewriteRule ^([a-z]+)/([0-9]+)/([0-9]+)$ operacion.php?op=$1&op1=$2&op2=$3
#http://localhost/EMECUBO/API/dame/3/4/4
RewriteRule ^([a-z]+)/([0-9]+)/([0-9]+)/([0-9]+)$ operacion.php?op=$1&op1=$2&op2=$3&op3=$4
RewriteRule ^([a-z]+)/([a-z]+)/([0-9]+)$ operacion.php?op=$1&op1=$2&op2=$3
